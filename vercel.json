{
  "builds": [
    {
      "src": "/**",
      "use": "@vercel/static"
    },
    {
      "src": "index.mjs",
      "use": "@vercel/node@canary"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "(.*)",
      "destination": "/index.mjs"
    }
  ],
  "github": {
    "silent": true
  }
}
